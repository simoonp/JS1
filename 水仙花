//abc = 'Hello, JS';
//alert(abc);
var life={};
for(life.age=1;life.age<=5;life.age++)
{
	switch(life.age)
	{
		case 1:
		  life.body = "卵细胞" ; //增加body属性
		  life.say = function(){alert(this.age+this.body)};//新建say方法
		  break;
		case 2:
		  life.tail = "尾巴";//增加tail属性
		  life.gill ="腮";//增加gill属性
		  life.body = "蝌蚪";
		  life.say = function(){alert(this.age+this.body+"-"+this.tail+"，"+this.gill)};
		  break;
		case 3:

		                 delete life.tail;//删除ail属性
		                 delete life.gill;//删除gill属性
		                 life.legs = "四条腿";//增加legs属性
		                 life.lung = "肺"; //增加lung属性
		                 life.body = "青蛙";
		                 life.say = function(){alert(this.age+this.body+"-"+this.legs+"，"+this.lung)};
		                 break;
		case 4:
		                 delete life.tail;
		                 delete life.gill;
		                 delete life.legs;
		                 delete life.lung;
                         delete life.body;
                         life.love = "找到挚爱";
                         life.say = function(){alert(this.age+this.love)};
                         break;
        case 5:
                         delete life.tail;
		                 delete life.gill;
		                 delete life.legs;
		                 delete life.lung;
                         delete life.body;
                         delete life.love;
                         life.regeneration = "变回王子";
                         life.say = function(){alert(this.regeneration)};
                         break;
	};
	life.say();//调用say方法
}
var myfunc = function()
{
	alert("hello");
};
alert(typeof(myfunc));

var myfunc = function()
{
	alert("hello");
};
myfunc();//第一次调用myfunc,输出hello

myfunc = function()
{
	alert("yerh");
};
myfunc();//第二次调用myfunc,输出yeah

myfunc = function()
{
	alert("hello");
};
myfunc();//输出的还是hello和“悟透JS”上讲的不一样

myfunc = function()
{
	alert("yerh");
};
myfunc();
var a=153;
    d=a%10;
    c=(a%100-d)/10;
    b=(a-c*10-d)/100;
document.write(a+'<br/>'+b+'<br/>'+c+'<br/>'+d);
