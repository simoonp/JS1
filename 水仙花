var i,j;
    for(i=1;i<10;i++) {
        for(j=1;j<=i;j++) {
        document.write(j+'*'+i+'='+i*j+'&nbsp;');
        }
    document.write('<br/>');
        }
document.write('<br/>');
 var i,j;
    for(i=1;i<10;i++) {
        for(j=1;j<=i;j++) {
        document.write(j+'*'+i+'='+i*j+'&nbsp;');
        }
    document.write('<br/>');
        }
document.write('<br/>');
    var a,b,c,d,e;

    for (a=100;a<1000;a++)
   {
    	b=a/100;
    	c=a/10-b*10;
    	d=a%10;
    	e=b*b*b+c*c*c+d*d*d-a;
    	if(e==0)
    	{
    		document.write(a+'是水仙花数'+'&nbsp;');
    	    document.write('<br>');
    	}
    }//Js中没有整型和浮点型之分，以上b,c为小数，if的判断条件不成立，无法输出
    var a,b,c,d;
    for(a=100;a<1000;a++)
    	{
        d=a%10;
        c=(a%100-d)/10;
        b=(a-c*10-d)/100;
        if(b*b*b+c*c*c+d*d*d==a)
        {
        	document.write(a+'是水仙花数'+'<br/>');
        }
        }//可以编译
    	var a=370;
        d=a%10;
        c=(a%100-d)/10;
        b=(a-c*10-d)/100;
        if(b*b*b+c*c*c+d*d*d==a)
        {
        	document.write(a+'是水仙花数'+'<br/>');
        }
        document.write(a+'<br/>'+b+'<br/>'+c+'<br/>'+d);
var a, b, c,i;

　　　　for(a=1;a<10;a++){  //提取百位
                    for(b=0;b<10;b++){  //提取十位
                        for(c=0;c<10;c++) { //提取个位
                            var i = a*100+b*10+c;
                            if (i=== a*a*a +b*b*b + c*c*c){
                                document.write(i+'是水仙花数'+'<br/>');
                            }
                        }
                    }
                }

